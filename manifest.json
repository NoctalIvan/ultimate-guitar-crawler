{
  "name": "Ultimate guitar crawler",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Ultimate guitar playlist crawler extension for google chrome",
  "homepage_url": "https://github.com/NoctalIvan/ultimate-guitar-crawler",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Ultimate guitar crawler",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "contentSettings",
    "https://www.ultimate-guitar.com/*"
  ]
}